<%- include('partials/header') %>
    <div class="page-wrapper bg-gra-01 p-t-180 p-b-100 font-poppins">
        <div class="wrapper wrapper--wfull">
            <div class="card card-3">
                <div class="card-heading_reg"></div>
                <div class="card-body">
                    <h2 class="title" style="text-align: center">Create Your Movement!</h2>
                    <form action="/protest/new" method="POST">
                        <div class="input-group">
                            <input class="input--style-3" type="text" name="name" placeholder="Name" required>
                        </div>
                        <div class="input-group">
                            <input class="input--style-3" type="text" name="organization" placeholder="Organization"
                                required>
                        </div>
                        <div class="input-group">
                            <input class="input--style-3" type="date" name="date" placeholder="Date" required>
                        </div>
                        <div class="input-group">
                            <input class="input--style-3" type="time" name="time" placeholder="Time" required>
                        </div>

                        <div class="input-group">
                            <input class="input--style-3" type="text" name="location" placeholder="Location" required>
                        </div>
                        <div class="input-group">
                            <textarea class="input--style-3" type="text" name="description"
                                placeholder="Description (Markdown Supported)" rows="6" cols="50" required></textarea>
                        </div>
                        <div class="input-group">
                            <textarea class="input--style-3" type="text" name="keywords" cols="50"
                                placeholder="Keywords/Tags" required></textarea>
                        </div>
                        <div class="p-t-10 text-center">
                            <button class="btn btn--pill btn--green" type="submit" value="Start">Launch</button>
                        </div>
                    </form>
                </div>
            </div>
        </div>
    </div>

